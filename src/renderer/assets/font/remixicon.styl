@font-face
  font-family: 'remixicon'
  src: url('remixicon.woff2?t=1590207869815') format('woff2')
  font-display: swap

[class^='ri-'], [class*=' ri-']
  font-family: 'remixicon' !important
  font-style: normal
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

.ri-left, .ri-down
  font-size: 24px

.ri-refresh
  font-size: 18px

.ri-close
  font-size: 20px

.ri-headset
  font-size: 14px

.ri-play-circle.play-btn
  font-size: 26px

